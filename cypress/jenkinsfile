pipeline {
    agent any
    stages {
        stage('Setup'){
            steps {
                git branch: 'main', url: 'https://github.com/TGregorio11/Exercicio-Aula-Cypress-2.git'
            }
        }
        stage('Instalar dependencias'){
            steps {
                bat'npm install'
            }
        }
        stage ('Executar Testes') {
    steps{
        bat '''
        npm run cy:run'''
    }
       }
    }
}